{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/no-v-model-argument": "off",
        "space-before-blocks": "off",
        "quotes": [2, "single", { "avoidEscape": true }],
        "no-extra-semi": "off",
        "semi": [ "error", "never" ],
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "always",
            "asyncArrow": "always"
        }]
//        "no-unused-vars": "off"
    }
    // "parser": "@babel/eslint-parser"
}
